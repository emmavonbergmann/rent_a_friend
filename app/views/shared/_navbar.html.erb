<div class="navbar navbar-expand-sm navbar-light navbar-airbnb">
  <div class="container-fluid">
    <%= link_to "#", class: "navbar-brand" do %>
      <%= image_tag "https://img.freepik.com/premium-vector/community-logo-design-vector_67715-512.jpg?w=2000" %>
    <% end %>

    <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="buttons">
      <button class="search-your-friend"><%= link_to "Search your friend", "#", class: "btn btn-ghost" %></button>
    </div>
    <div class="secondpart">
      <ul class="navbar-nav me-auto">
          <li class="nav-item dropdown">
            <%= image_tag "https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcRjoXyX2SJzeszHi_uHo-S0kWJKjbVhIB4atb1g7LxIp1wdjqrP9Bz7zIHBVBnmBuJorFc&usqp=CAU", class: "avatar dropdown-toggle", id: "navbarDropdown", data: { bs_toggle: "dropdown" }, 'aria-haspopup': true, 'aria-expanded': false %>
            <div class="dropdown-menu dropdown-menu-end">
              <% if current_user %>
                  <%= link_to "My Profile", profile_path(current_user), class: "dropdown-item" %>
                  <%= link_to "Log out", destroy_user_session_path, class: "dropdown-item", data: {turbo_method: :delete} %>
                <% else %>
                  <%= link_to "Sign Up", new_user_registration_path, class: "dropdown-item" %>
                  <%= link_to "Log in", new_user_session_path, class: "dropdown-item" %>
              <% end %>
            </div>
          </li>
      </ul>
    </div>
  </div>
</div>
